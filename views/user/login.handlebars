<div class="container d-flex justify-content-center" style="background-color: #f3efe3;">
<div class="col-md-7">
  <br>
  <center><h3>Logar-se</h3></center>
  <br>
  <form>
    <div class="form-group">
      <label for="labelEmail">Email</label>
      <input type="email" class="form-control" id="inputEmail" aria-describedby="email" placeholder="Seu email">
    </div>
    <div class="form-group">
      <label for="labelSenha">Senha</label>
      <input type="password" class="form-control" id="inputSenha" placeholder="Senha">
    </div>
    <div class="form-row">
      <div class="form-group col-md-"2>
        <button type="button" class="btn btn-dark">Entrar</button>
      </div>
      <div class="form-group col-md-3">
          <a href="/cadastrar"><button type="button" class="btn btn-warning">Cadastre-se</button></a>
      </div>
    </div>
    <br>
    <br>
  </form>

</div>